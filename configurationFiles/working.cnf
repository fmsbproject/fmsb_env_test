[ req ]
default_bits        = 3072
default_md          = sha256
distinguished_name  = req_distinguished_name
req_extensions      = v3_req
 
[ req_distinguished_name ]
CN                  = Unique UUID
 
[ v3_req ]
basicConstraints    = CA:FALSE
keyUsage            = critical, digitalSignature, keyEncipherment, dataEncipherment
extendedKeyUsage    = serverAuth, clientAuth
 
subjectAltName      = @alt_names_server, @alt_names_client
2.16.726.1.1.1.1 = ASN1:UTF8String:Message Gateway Protocol Version 1
2.16.726.1.1.1.2 = ASN1:UTF8String:Business Service RTGS
2.16.726.1.1.1.3 = ASN1:UTF8String:Participant POST
 
[ server_cert ]
basicConstraints = CA:FALSE
keyUsage = critical, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alt_names_server
 
[ alt_names_server ]
IP.1 = 16.171.72.189
IP.2 = 192.168.100.132
DNS.1 = DESKTOP-ULAT27D
DNS.2 = 090002
DNS.3 = RTGS
DNS.4 = 1.0
 
[ client_cert ]
basicConstraints = CA:FALSE
keyUsage = critical, digitalSignature, keyEncipherment
extendedKeyUsage = serverAuth, clientAuth
subjectAltName = @alt_names_client
 
[ alt_names_client ]
IP.1 = 192.168.100.73
IP.2 = 192.168.100.132
DNS.1 = DESKTOP-QLCUV89
DNS.2 = 090002
DNS.3 = RTGS
DNS.4 = 1.0
 
[ code_signing_cert ]
basicConstraints = CA:FALSE
keyUsage = critical, digitalSignature
extendedKeyUsage = codeSigning
 
[ email_cert ]
basicConstraints = CA:FALSE
keyUsage = critical, digitalSignature,keyEncipherment, dataEncipherment
extendedKeyUsage = emailProtection


